{
  "name": "angular1.5-seed",
  "version": "1.0.0",
  "description": "Angular 1 that is ready for Angular 2 ",
  "main": "./src/index.js",
  "scripts": {
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/index.html src/favicon.ico dist",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "set NODE_ENV=dev && webpack-dev-server --open --history-api-fallback --content-base dist",
    "prebuild": "npm run clean-and-copy",
    "build": "set NODE_ENV=dev && webpack",
    "prebuild:prod": "npm run clean-and-copy",
    "build:prod": "set NODE_ENV=dev && webpack",
    "test": "set NODE_ENV=dev && karma start test/karma.conf.js"
  },
  "keywords": [
    "AngularJS",
    "ES6",
    "Webpack"
  ],
  "author": "Francois Venter <francois@tangentsolutions.co.za>",
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "ng-annotate-loader": "^0.1.1",
    "node-sass": "^3.10.0",
    "phantomjs-prebuilt": "^2.1.12",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-cookies": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "angular-toastr": "^2.1.1",
    "angular-touch": "^1.5.8",
    "angular-ui-bootstrap": "^2.1.3",
    "angular-ui-router": "^1.0.0-beta.2",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "mobx": "^2.5.1",
    "rx-angular": "^1.1.3",
    "toastr": "^2.1.2",
    "underscore": "^1.8.3"
  }
}
